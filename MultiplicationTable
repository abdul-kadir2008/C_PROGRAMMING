/* MultiplicationTable
Write a C program that prompts the user to enter a number
 and then prints the multiplication table for that number
  using a while loop.
*/
#include<stdio.h>
int main(){
	int n,i=1;
	printf("Multiplication Table:");
	printf("\nEnter Your Number for Table:");
	scanf("%d",&n);
	while(i<=10)
	{
		printf("\n%d x %d = %d",n,i,n*i);
		i=i+1;
	}
	return 0;
}
