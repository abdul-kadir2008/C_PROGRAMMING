/*Q3.

In a shop, prices of 5 items are given. Write a program to calculate the total bill amount.*/
#include<stdio.h>
int main()
{
	int price[5];
	int Total=0;
	for(int i=0;i<5;i++)
	{
		printf("Enter Price Of Item %d :",i+1);
	    scanf("%d",&price[i]);
	    Total+=price[i];
	
	}
	printf("Total bill of Five items = %d",Total);
	return 0;
}
