/*
Write a program to input elements in a 3Ã—3 matrix and print the maximum element in the matrix.
*/
#include<stdio.h>
int main()
{
	int element[3][3],max;
	scanf("%d",&element[0][0]);
	max=element[0][0];
	for(int i=0;i<3;i++)
	{
		for(int j=(i==0?1:0);j<3;j++)   //before loop input one element so using condisnal oprater:
		{
			scanf("%d",&element[i][j]);
			if(element[i][j] > max)
			{
				max=element[i][j];
			}
		}
		
		
	}
	printf("Max Number %d",max);
	return 0;
}
