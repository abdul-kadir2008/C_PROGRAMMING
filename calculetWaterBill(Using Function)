#include<stdio.h>
int calculetWaterBill(int Liter)
{
	int result=1,n=Liter; 
	for(int i=1;i<=n;i++)
	if (i>1&&i<=2)
	{
		result=result+2;
	}
	else if (i>2&&i<=5)
	{
		result=result+4;
	}
	else if(i>5)
	{
		result = result+10;
	}
       
return result;
}
int main()
{
	int a;
	printf("Enter used Water In Liters:");
	scanf("%d",&a);
	printf("Liter");
	int b =calculetWaterBill(a);
	printf("\nTotal Water Bill : %d rs.",b);
	return 0;
}
