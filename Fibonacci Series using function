/*. Fibonacci Series
 Write a function void printFibonacci(int n).
 Example 1: 5 ? 0 1 1 2 3
 Example 2: 7 ? 0 1 1 2 3 5 8
 */
 #include<stdio.h>
 void printFibonacci(int n)
 {
 	int a=0,b=1,c;
 	
 	printf("%d %d ",a,b);
 	for(int i=2;i<n;i++)
 	{
 		c=a+b;
 		printf("%d ",c);
 		a=b;
 		b=c;
	 }
 }
 int main()
 {
 	int num;
 	printf("Enter How many term:");
 	scanf("%d",&num);
 	printf("Fibonacci Series:");
 	printFibonacci(num);
 	return 0;
 }
