/*Q1.

Ek program likho jo 7 dinon ke temperature input le aur unka average temperature print kare.
*/
#include<stdio.h>
int main()
{
    float avgtemp[7];
	float total;
	for(int i=0;i<7;i++)
	{
		printf("Enter temperature Day %d ",i+1);
		scanf("%f",&avgtemp[i]);

		total+=avgtemp[i];
	}
	float result=total/7;
	printf("avrage temperature for weak:%.2fÂ°C",result);
	return 0;
}
